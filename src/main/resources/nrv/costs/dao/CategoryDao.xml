<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="nrv.costs.dao.CategoryDao">

    <insert id="insert" parameterType="nrv.costs.domain.Category" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        insert into category(type, tags)
        values(#{type}, #{tags})
    </insert>

</mapper>